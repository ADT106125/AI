   "source": [
    "#檢查質數\n",
    "n=int(input(\"請輸入質數\\n\"))\n",
    "if(n==2):\n",
    "    print(\"2是質數\")\n",
    "else:\n",
    "    for fe in range(2,n):\n",
    "        if(n%fe==0):\n",
    "            print(str(n)+\"不是質數\")\n",
    "            break\n",
    "    else:\n",
    "        print(str(n)+\"是質數\")\n",
    "    \n"
   ]
  }
